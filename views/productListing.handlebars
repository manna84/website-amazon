<section class="product-box row">

    <article class="product-bar-list">

        <ul>
            <li><h5>Electronics</h5></li>
            <li><a href="">Audio & Home Theatre</a></li>
            <li><a href="">Camera,photo & video</a></li>
            <li><a href="">GPS & Navigation</a></li>
            <li><a href="">Office Electronics</a></li>
            <li><a href="">Accessories & Supplies</a></li>
            <li><a href="">Computers & Accessories</a></li>
            <li><a href="">eBook Reader & Accessories</a></li>
            <li><a href="">Cell Phone & Accessories</a></li>
            <li><a href="">Headphones</a></li>
            <li><a href="">Portable Audio & Video</a></li>
            <li><a href="">Radio Communication</a></li>
            <li><a href="">Security and surveillance</a></li>
            <li><a href="">Telephone and Accessories</a></li>
            <li><a href="">Televisions & Video</a></li>
        </ul>

    </article>

    <article class="product-best">
        
        {{#each data}}
        <article class="product-view">

            <ul>
                <li><a href="/product-detail/{{this.id}}"><img src="./uploads/{{this.productimg}}" alt="camera" class="product-photo"></a></li>
                <li><a href="/product-detail/{{this.id}}" class="product-desc">{{this.name}}</a></li>
                <li>
                    <ul class="rating-box">
                        <li><img src="./img/rating-4.png" alt="star rating" class="rating"></li>
                        <li>123</li>
                    </ul>
                </li>
            
                <li>
                    <ul class="product-price">
                        <li><a href="/product-detail/{{this.id}}">CDN$ {{this.price}}</a></li>
                        <li><img src="./img/prime.png" alt="prime logo" class="prime-logo"></li>
                    </ul>
                <li>

                {{!-- <li class="btn-view"><a href="/product-detail/{{this.id}}">View Product</a></li> --}}
            </ul>
            
        </article>
        {{/each}}

    </article>
</section>